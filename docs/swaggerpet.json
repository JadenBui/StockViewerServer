{ "openapi":"3.0.0",
    "info":{ 
        "title":"World City API",
        "description":"This is a simple API based on World Cities API",
        "version":"1.0.0",
        "termsOfService":"http://swagger.io/terms/",
        "contact":{"email":"n10560564@qut.edu.au"},
        "license":{
            "name":"Apache 2.0",
            "url":"http://www.apache.org/licenses/LICENSE-2.0.html"
        } 
    },
    "servers":[ 
        {
            "url": "http://localhost:3000"
        }
    ],
    "basePath":"/api",
    "tags":[
        { 
          "name":"information",
          "description":"General API description"
        },
        { 
          "name":"query",
          "description":"API queries based on countries code"
        }
    ],
    "paths":{ 
        "/api":{ 
            "get":{ 
                "tags":["information"],
                "summary":"Simple information page",
                "description":"",
                "operationId":"apiHome",
                "produces":["text/html"],
                "responses":{
                    "200":{
                           "description":"Successful operation"
                          }
                }
            }
        },
        "/api/city":{ 
            "get":{
                "tags":["query"],
                "summary":"List of cities",
                "description":"Full listing the cities and their provinces for the database",
                "operationId":"listOfCitiesAndDistric",
                "produces":["application/json"],
                "responses":{
                    "200":{
                        "description":"successful operation",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "Error":{
                                    "type":"string",
                                    "enum":["true,false"]
                                },
                                "Message":{
                                    "type":"string",
                                    "enum":["Success","Failure"]
                                },
                                "City":{
                                    "type":"array",
                                    "items":{
                                        "type":"object",
                                        "properties":{
                                            "name":{
                                                "type":"string"
                                            },
                                            "distric":{
                                                "type":"string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400":{
                        "description":"Invalid status value"
                    }
                }
            }
        }
    }
}
                